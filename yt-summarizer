#!/usr/bin/env python3
"""
YouTube Video Summarizer - Main executable script

This script serves as the main entry point for the YouTube Video Summarizer CLI.
"""

import sys
import os

# Add the src directory to the Python path
sys.path.insert(0, os.path.join(os.path.dirname(__file__), 'src'))

from cli import cli

if __name__ == '__main__':
    cli()

